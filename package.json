{
  "name": "mybulma",
  "version": "1.0.0",
  "main": "sass/mystyles.scss",
  "license": "MIT",
  "dependencies": {
    "bulma": "^0.9.4",
    "node-sass": "^7.0.1",
    "clean-css-cli": "^5.4.2",
    "sass": "^1.52.1"
  },
  "scripts": {
    "css-build": "node-sass --omit-source-map-url sass/mystyles.scss css/mystyles.css",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch",
    "sass": "sass --watch assets/scss:assets/css",
    "compile-styles": "node-sass --output-style compressed assets/scss/mystyles.scss > assets/css/mystyles.css"
  }
}
